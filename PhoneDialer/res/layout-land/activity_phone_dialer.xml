<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.phonedialer.PhoneDialerActivity" 
    android:orientation="vertical">

    <EditText
        android:id="@+id/editText1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:gravity="center"
        android:inputType="phone"/>
    
<GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:rowCount="3"
        android:columnCount="5">
    
    <ImageButton
        android:id="@+id/delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/backspace" 
        android:background="@null"
        android:layout_row="2"
        android:layout_column="2"
        android:contentDescription="@string/none"
        android:layout_gravity="center"/>

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b1"
        android:layout_row="0"
        android:layout_column="0"/>
    
    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b2" 
        android:layout_row="0"
        android:layout_column="1"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b3" 
        android:layout_row="0"
        android:layout_column="2"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b4" 
        android:layout_row="0"
        android:layout_column="3"/>
    
    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b5" 
        android:layout_row="0"
        android:layout_column="4"/>
    
    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b6" 
        android:layout_row="1"
        android:layout_column="0"/>
    
    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b7" 
        android:layout_row="1"
        android:layout_column="1"/>
    
    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b8" 
        android:layout_row="1"
        android:layout_column="2"/>
    
    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b9" 
        android:layout_row="1"
        android:layout_column="3"/>
    
    <Button
        android:id="@+id/buttonstar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/star" 
        android:layout_row="2"
        android:layout_column="1"/>
    
    
    <Button
        android:id="@+id/button0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/b0" 
        android:layout_row="1"
        android:layout_column="4"/>
    
    <Button
        android:id="@+id/buttonhash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hash" 
        android:layout_row="2"
        android:layout_column="3"/>
    
     <ImageButton
            android:id="@+id/call_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/call"
            android:layout_gravity="center"
            android:layout_row="2"
            android:layout_column="0"
            android:contentDescription="@string/none"/>
        <ImageButton
            android:id="@+id/hangup_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/hangup"
            android:layout_gravity="center"
            android:layout_row="2"
            android:layout_column="4"
            android:contentDescription="@string/none"/>
    
     </GridLayout>

</LinearLayout>